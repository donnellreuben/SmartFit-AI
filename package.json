{
  "name": "smartfit-ai",
  "version": "0.0.1",
  "private": true,
  "description": "SmartFit AI - AI-powered personalized workout plans with equipment recognition and progress tracking",
  "keywords": [
    "react-native",
    "fitness",
    "ai",
    "workout",
    "health",
    "mobile"
  ],
  "author": "Donnell Reuben",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/donnellreuben/SmartFit-AI.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "start": "react-native start",
    "test": "jest",
    "web": "webpack serve --mode development",
    "web:build": "webpack --mode production",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace SmartFitAI.xcworkspace -scheme SmartFitAI -configuration Release -destination generic/platform=iOS -archivePath SmartFitAI.xcarchive archive"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.81.4",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "react": "19.1.0",
    "react-dom": "^19.1.1",
    "react-native": "0.81.4",
    "react-native-camera": "^4.2.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.16.0",
    "react-native-video": "^6.16.1",
    "react-native-web": "^0.21.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@expo/webpack-config": "^19.0.1",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "0.81.4",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.19.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.4",
    "jest": "^29.6.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "engines": {
    "node": ">=20"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
